{
  "name": "halcyon",
  "version": "1.0.0",
  "description": "halcyon",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.9.1",
    "autoprefixer": "^9.4.7",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.3",
    "del": "^3.0.0",
    "jquery": "^3.3.1",
    "npm": "^6.8.0",
    "pixel-glass": "^1.0.13",
    "popper.js": "^1.14.7",
    "run-sequence": "^2.2.1",
    "stylelint": "^9.10.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-js-minify": "0.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "posthtml-include": "^1.2.0"
  }
}
